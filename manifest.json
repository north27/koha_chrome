{
    "name": "Koha",
    "version": "1.3.2",
    "description": "Provide search results from Koha alongside Google results",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_title": "Koha search by North27",
        "default_icon": "icon128.png"
    },
    "content_scripts": [
        {
          "matches": ["*://*.google.com/search*", "*://*.google.co.uk/search*"],
          "js": ["koha.js"]
        }
      ],
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
  }